<script type='text/javascript'>
    (function ($) {
        $(function () {
            $(document).ready(function ($) {
                $('.maskTelefone').mask('(99) 99999-9999');
                $('.maskCPF').mask('999.999.999-99');
                $('.maskRG').mask('99.999.999');

                $('#nome').focus();
            });
        });
    })(jQuery);

    $("#tipoUsuarioMedico").click(function () {
        if ($('#tipoUsuarioRecepcionista').is(":checked")) {
            $("#tipoUsuarioRecepcionista").prop("checked", false);
        }
    });

    $("#tipoUsuarioRecepcionista").click(function () {
        if ($('#tipoUsuarioMedico').is(":checked")) {
            $("#tipoUsuarioMedico").prop("checked", false);
        }
    }); 

    $('#btnSelecionarTodos').click(function () {
        $("input[type='checkbox']").prop("checked", true);
    });

    $('#moveProximoDadosPessoais').click(function () {
        var target = $(".nav-tabs li.active");
        var sibbling = target.next();
        sibbling.children("a").tab("show");
    });

    $('#moveAnteriorDadosLogin').click(function () {
        var target = $(".nav-tabs li.active");
        var sibbling = target.prev();
        sibbling.children("a").tab("show");
    });

    $('#moveProximoDadosLogin').click(function () {
        var target = $(".nav-tabs li.active");
        var sibbling = target.next();
        sibbling.children("a").tab("show");
    });

    $('#moveAnteriorDadosPermissoes').click(function () {
        var target = $(".nav-tabs li.active");
        var sibbling = target.prev();
        sibbling.children("a").tab("show");
    });
</script>