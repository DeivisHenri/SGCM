            <td>@ViewBag.FlUsuarioC</td>
            <td>0</td>
        } else {
            <td>0</td>
            <td>@ViewBag.FlUsuarioC</td>
        }
    </tr>
    <tr>
        <td>Alterar Usuário</td>
        @if (ViewBag.FlUsuarioA == 1)
        {
            <td>@ViewBag.FlUsuarioA</td>
            <td>0</td>
        } else {
            <td>0</td>
            <td>@ViewBag.FlUsuarioA</td>
        }
    </tr>
    <tr>
        <td>Excluir Usuário</td>
        @if (ViewBag.FlUsuarioE == 1)
        {
            <td>@ViewBag.FlUsuarioE</td>
            <td>0</td>
        } else {
            <td>0</td>
            <td>@ViewBag.FlUsuarioE</td>
        }
    </tr>
    <tr>
        <td>Laughing Bacchus Winecellars</td>
