<h2>Session</h2>

<h4>ID: @ViewBag.ID - Username: @ViewBag.Username - Password: @ViewBag.Password - Tipo Usuário: @ViewBag.TipoUsuario</h4>

<table border="1">
    <tr>
        <th align="center">Nome da Flag</th>
        <th align="center">Sim</th>
        <th align="center">Não</th>
    </tr>
    <tr>
        <td>Inserir Usuário</td>
        @if (ViewBag.FlUsuarioI == 1)
        {
            <td>@ViewBag.FlUsuarioI</td>
            <td>0</td>
        } else {
            <td>0</td>
            <td>@ViewBag.FlUsuarioI</td>
        }
    </tr>
    <tr>
        <td>Consultar Usuário</td>
        @if (ViewBag.FlUsuarioC == 1)
